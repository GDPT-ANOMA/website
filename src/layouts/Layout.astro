---
import navigationTabs from "../data/navigationTabs.json";
import { Icon } from "astro-icon/components";
import "../styles/hamburgers.css";
interface Props {
    title: string;
    class: string;
}

const { title, class: className } = Astro.props;
---

<!doctype html>
<html lang="en">
    <head>
        <link href="../styles/hamburgers.css" rel="stylesheet" />
        <meta charset="UTF-8" />
        <meta name="description" content="A website for GĐPT Anôma" />
        <meta name="viewport" content="width=device-width, initial-scale=1.0" />
        <link rel="icon" type="image/svg+xml" href="/logos/gdpt.svg" />
        <title>{title}</title>
    </head>
    <body
        class="bg-grayBackground max-w-screen-2xl px-4 sm:px-6 lg:px-8 mx-auto"
    >
        <!-- Main Body -->
        <main class="my-4 mx-4">
            <slot />
        </main>
    </body>
</html>
